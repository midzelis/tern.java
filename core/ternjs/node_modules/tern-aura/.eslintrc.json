{
  "env": {
    "node": true,
    "es6": true
},
    "extends": [
        "eslint:recommended"
    ],

    "rules": {
        "no-set-state": "off",
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}